.text-line-1 {
	display: -webkit-box;
	word-break: break-all;
	/*超出文字用省略号代替*/
	text-overflow: ellipsis;
	/*竖直方向的超出和隐藏*/
	-webkit-box-orient: vertical;
	/*设定行数为2*/
	-webkit-line-clamp: 1;
	/*多出部分隐藏*/
	overflow: hidden;
}
.text-line-2 {
	display: -webkit-box;
	word-break: break-all;
	/*超出文字用省略号代替*/
	text-overflow: ellipsis;
	/*竖直方向的超出和隐藏*/
	-webkit-box-orient: vertical;
	/*设定行数为2*/
	-webkit-line-clamp: 2;
	/*多出部分隐藏*/
	overflow: hidden;
}

