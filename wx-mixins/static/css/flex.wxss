.popUp {
	width: 100vw;
	height: 100vh;
	background-color: rgba(0, 0, 0, 0.4);
	position: fixed;
	left: 0;
	bottom: 0;
	z-index: 999999;
	overflow: hidden;
}
.pop-animation{
	animation: pop 0.3s ease ;
}

@keyframes pop{
	from{
		height: 0rpx;
	}
	to{
		height: 100vh;
	}
}

